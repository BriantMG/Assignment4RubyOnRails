<p style="color: green"><%= notice %></p>
<div id="products">
  <%= render @product %>
  <div>
    <%= link_to " Edit", edit_product_path(@product), class: "bi bi-pen btn btn-primary" %> |
    <%= link_to " Back", products_path, class: "bi bi-arrow-left btn btn-primary" %>
    <%= button_to " Destroy", @product, method: :delete , class: "bi bi-trash btn btn-danger" %>
  </div>
</div>
